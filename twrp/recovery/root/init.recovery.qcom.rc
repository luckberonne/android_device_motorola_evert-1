on fs
    wait /dev/block/platform/soc/c0c4000.sdhci
    symlink /dev/block/platform/soc/c0c4000.sdhci /dev/block/bootdevice
    symlink /dev/block/bootdevice/by-name/oem_b /dev/block/bootdevice/by-name/vendor_b
    symlink /dev/block/bootdevice/by-name/oem_a /dev/block/bootdevice/by-name/vendor_a
    symlink /dev/block/bootdevice/by-name/oem /dev/block/bootdevice/by-name/vendor
    chmod 0660 /dev/qseecom
    write /sys/class/power_supply/usb/otg_switch 1
    chown system drmrpc /dev/qseecom
    chmod 0664 /dev/ion
    chown system system /dev/ion
    export LD_LIBRARY_PATH /sbin:/vendor/lib64:/vendor/lib64/hw

# separate copy needed to use /sbin/linker64 instead of /system/bin/linker64

service prepdecrypt /sbin/prepdecrypt.sh
    oneshot
    disabled
    user root
    group root
    seclabel u:r:recovery:s0

service boot-1-0 /sbin/android.hardware.boot@1.0-service
    user root
    group root
    disabled
    seclabel u:r:recovery:s0

service servicemanager /sbin/servicemanager
    user root
    group root readproc
    disabled
    seclabel u:r:recovery:s0


# keystore is started and stopped on demand by TWRP


service time_daemon /sbin/time_daemon
    user root
    group root
    disabled
    seclabel u:r:recovery:s0

on boot
    # save battery by switching to interactive once booted
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor interactive
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor interactive
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor interactive
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_governor interactive

on init
    setprop ro.hardware.keystore sdm660
    setprop ro.hardware.gatekeeper sdm660
    setprop ro.hardware.bootctrl sdm660
    setprop ro.build.system_root_image true
    start prepdecrypt
    start time_daemon

# Only needed when we don't include all required blobs for crypto
    start prepdecrypt

service prepdecrypt /sbin/init.prepdecrypt.sh
    oneshot
    user root
    group root
    disabled
    seclabel u:r:recovery:s0